    "import numpy as np\n",
    "\n",
    "# Creating a NumPy array\n",
    "arr = np.array([12, 4, 56, 7, 29, 10])\n",
    "\n",
    "# Sorting the array\n",
    "sorted_arr = np.sort(arr)\n",
    "\n",
    "# Display results\n",
    "print(\"Original array:\", arr)\n",
    "print(\"Sorted array:\", sorted_arr)\n"
